<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BlzBundle\Entity\UserData" table="user_data">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rg" type="string" column="rg" length="9" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cpf" type="string" column="cpf" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="celphone" type="string" column="celphone" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="social" type="boolean" column="social" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="photo" type="string" column="photo" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="push" type="string" column="push" length="36" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
